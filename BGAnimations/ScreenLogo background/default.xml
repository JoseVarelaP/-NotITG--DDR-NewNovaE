<ActorFrame><children>
	<!-- This will act as the BGAnimation LengthSeconds command. -->
	<Layer Type="Quad" OnCommand="%function(self) self:zoom(0) self:sleep(8) end" />

	<Layer File="1.png" OnCommand="zoomto,SCREEN_WIDTH,SCREEN_HEIGHT;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y" />
	<Layer File="sphere.png" OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y+(238-240)" />
	<Layer File="effect.png" OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoom,0.96;glowshift;spin;effectmagnitude,0,0,-65;" />
	<Layer File="effect2.png" OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;rotationz,0.5;zoom,0.96;glowshift;spin;effectmagnitude,0,0,55;" />
	<Layer File="logo.png" OnCommand="x,SCREEN_CENTER_X+(311-320);y,SCREEN_CENTER_Y+(226-240)" />

</children></ActorFrame>